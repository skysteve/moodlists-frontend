{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom",
      "webworker"
    ],
    "moduleResolution": "node",
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "jsx": "react"
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
