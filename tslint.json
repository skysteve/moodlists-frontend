{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": [true, "always"],
    "prefer-const": true,
    "trailing-comma": true,
    "max-line-length": false,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  }
}
